package Implementation;

public class SupermarketArray  implements supermaket{

	private final String[] itens;
	
	private int lastindex;
	
	public  SupermarketArray(final int size) {
		itens = new String[size];
		lastindex = -1;
	}
	
	
	
	@Override
	public void add(String item) {
		if(lastindex == itens.length-1);{
			System.err.print("Lista de supermercado cheia");
		}else {
			lastindex++;
			itens[lastindex] =item;
		}
	}

	@Override
	public void print() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void delete(int index) {
		// TODO Auto-generated method stub
		
	}
	

}
